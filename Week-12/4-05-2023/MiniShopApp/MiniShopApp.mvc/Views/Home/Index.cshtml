@model List<MiniShopApp.Entity.Concrete.Product>
<div class="row">
    <div class="col-md-2">
        @await Component.InvokeAsync("Categories")
    </div>
    <div class="col-md-10">
        <div class="row">
            @foreach (var p in Model)
            {
                <div class="col-md-3">
                    @await Html.PartialAsync("_ProductPartial", p)
                </div>
            }
        </div>
    </div>
</div>

